<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 AUTOSAR_00048.xsd">
  <ADMIN-DATA>
    <DOC-REVISIONS>
      <DOC-REVISION>
        <ISSUED-BY>RB</ISSUED-BY>
        <DATE>2022-11-10</DATE>
      </DOC-REVISION>
    </DOC-REVISIONS>
  </ADMIN-DATA>
  <AR-PACKAGES>
    <AR-PACKAGE>
      <SHORT-NAME>AUTOSAR_Port</SHORT-NAME>
      <AR-PACKAGES>
        <AR-PACKAGE>
          <SHORT-NAME>EcucModuleDefs</SHORT-NAME>
          <ELEMENTS>
            <!-- Module Definition: Port -->
            <ECUC-MODULE-DEF>
              <SHORT-NAME>Port</SHORT-NAME>
              <LONG-NAME>
                <L-4 L="EN">MCAL Port Driver</L-4>
              </LONG-NAME>
              <DESC>
                <L-2 L="EN">Configuration of the MCAL module for uC ports.</L-2>
              </DESC>
              <CATEGORY>VENDOR_SPECIFIC_MODULE_DEFINITION</CATEGORY>
              <ADMIN-DATA>
                <DOC-REVISIONS>
                  <DOC-REVISION>
                    <REVISION-LABEL>4.5.0</REVISION-LABEL>
                    <ISSUED-BY>AUTOSAR</ISSUED-BY>
                    <DATE>2020-11-26</DATE>
                  </DOC-REVISION>
                  <DOC-REVISION>
                    <REVISION-LABEL>4.0.0</REVISION-LABEL>
                    <ISSUED-BY>RB</ISSUED-BY>
                    <DATE>2020-10-23</DATE>
                  </DOC-REVISION>
                </DOC-REVISIONS>
              </ADMIN-DATA>
              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
              <POST-BUILD-VARIANT-SUPPORT>true</POST-BUILD-VARIANT-SUPPORT>
              <REFINED-MODULE-DEF-REF DEST="ECUC-MODULE-DEF">/AUTOSAR/EcucDefs/Port</REFINED-MODULE-DEF-REF>
              <SUPPORTED-CONFIG-VARIANTS>
                <SUPPORTED-CONFIG-VARIANT>VARIANT-PRE-COMPILE</SUPPORTED-CONFIG-VARIANT>
              </SUPPORTED-CONFIG-VARIANTS>
              <CONTAINERS>
                <!-- Container Definition: PortConfigSet -->
                <ECUC-PARAM-CONF-CONTAINER-DEF>
                  <SHORT-NAME>PortConfigSet</SHORT-NAME>
                  <LONG-NAME>
                    <L-4 L="EN">Configuration data set</L-4>
                  </LONG-NAME>
                  <DESC>
                    <L-2 L="EN">Defines one data set of post-build configuration. Which of these data sets is actually active is controlled by EcuM. Data sets can only be switched at initialization time.</L-2>
                  </DESC>
                  <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                  <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                  <SUB-CONTAINERS>
                    <!-- Container Definition: PortContainer -->
                    <ECUC-PARAM-CONF-CONTAINER-DEF>
                      <SHORT-NAME>PortContainer</SHORT-NAME>
                      <LONG-NAME>
                        <L-4 L="EN">Port Container</L-4>
                      </LONG-NAME>
                      <DESC>
                        <L-2 L="EN">Configuration of a single port, consisting of pins.<BR/>
The scheme to be used is uC dependent: Port_&lt;PortIdx&gt;.<BR/>
&lt;PortIdx&gt; specifies the index of the uC port.<BR/>
PortIdx for ST uC: &lt;A-Z&gt;<BR/>
PortIdx for IFX uC: &lt;00,01,...10,11...&gt;</L-2>
                      </DESC>
                      <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                      <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                      <MULTIPLICITY-CONFIG-CLASSES>
                        <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                          <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                          <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                        </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                      </MULTIPLICITY-CONFIG-CLASSES>
                      <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                      <SUB-CONTAINERS>
                        <!-- Container Definition: PortPin -->
                        <ECUC-PARAM-CONF-CONTAINER-DEF>
                          <SHORT-NAME>PortPin</SHORT-NAME>
                          <LONG-NAME>
                            <L-4 L="EN">Pin Container</L-4>
                          </LONG-NAME>
                          <DESC>
                            <L-2 L="EN">Configuration of an individual Port pin.</L-2>
                          </DESC>
                          <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                          <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                          <MULTIPLICITY-CONFIG-CLASSES>
                            <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                              <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                            </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                          </MULTIPLICITY-CONFIG-CLASSES>
                          <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                          <PARAMETERS>
                            <!-- PARAMETER DEFINITION: PortPinDirection -->
                            <ECUC-ENUMERATION-PARAM-DEF>
                              <SHORT-NAME>PortPinDirection</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Direction</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Configuration of the initial Port pin direction (In or Out).</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <LITERALS>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_DIRECTION_IN</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">In</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_DIRECTION_OUT</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">Out</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                              </LITERALS>
                            </ECUC-ENUMERATION-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinDirectionChangeable -->
                            <ECUC-BOOLEAN-PARAM-DEF>
                              <SHORT-NAME>PortPinDirectionChangeable</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Direction changeable</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Enables/disables if a pin direction is changeable by Port_SetPinDirection API call<BR/>
<BR/>
If set to true there needs to be at least one input mode and one output mode configured for this pin container.</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <DEFAULT-VALUE>false</DEFAULT-VALUE>
                            </ECUC-BOOLEAN-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinInitialMode -->
                            <ECUC-STRING-PARAM-DEF>
                              <SHORT-NAME>PortPinInitialMode</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Initial mode</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Configuration of the initial port pin mode. This mode will be active after Port_Init() function is called. The initial mode needs to be configured also as PortPinMode (PortPinMode contains all available modes and PortPinInitialMode selects the one which is initialized)<BR/>
<BR/>
Valid names are<BR/>
PORT_PIN_MODE_INPUT_xxx<BR/>
PORT_PIN_MODE_OUTPUT_xxx<BR/>
<BR/>
Since xxx depends on the current Machine Type please have a look at: <BR/>
Port_Cfg.h (only available after successful SW build)<BR/>
or<BR/>
the JSON files in Port/src-java/port/devices/[ifx/st]<BR/>
<BR/>
There you can find all supported modes for the current Machine Type. If you configure an invalid string here the error message will also tell you which modes are supported on the current pin for the current Machine Type.</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <ECUC-STRING-PARAM-DEF-VARIANTS>
                                <ECUC-STRING-PARAM-DEF-CONDITIONAL/>
                              </ECUC-STRING-PARAM-DEF-VARIANTS>
                            </ECUC-STRING-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinLevelValue -->
                            <ECUC-ENUMERATION-PARAM-DEF>
                              <SHORT-NAME>PortPinLevelValue</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Output Level</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Configuration of the initial pin output level. This tag is only needed if PORT_PIN_MODE_OUTPUT_DIO is present in PortPinMode tag. If a configuration has PORT_PIN_MODE_OUTPUT_DIO in the PortPinMode tag but the PortPinInitialMode tag contains another mode the PortPinLevelValue will be visible on the pin only after swichting the mode to PORT_PIN_MODE_OUTPUT_DIO.</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <LITERALS>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_LEVEL_HIGH</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">High</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_LEVEL_LOW</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">Low</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                              </LITERALS>
                            </ECUC-ENUMERATION-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinMode -->
                            <ECUC-STRING-PARAM-DEF>
                              <SHORT-NAME>PortPinMode</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Mode</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Configuration of the allowed port pin modes, which can be set by Port_SetPinMode API call. See tag PortPinInitialMode for possible values.</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>1</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY-INFINITE>true</UPPER-MULTIPLICITY-INFINITE>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <ECUC-STRING-PARAM-DEF-VARIANTS>
                                <ECUC-STRING-PARAM-DEF-CONDITIONAL/>
                              </ECUC-STRING-PARAM-DEF-VARIANTS>
                            </ECUC-STRING-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinModeChangeable -->
                            <ECUC-BOOLEAN-PARAM-DEF>
                              <SHORT-NAME>PortPinModeChangeable</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Mode changeable</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Enables/disables if a pin mode is changeable by Port_SetPinMode API call. The possible modes which can be used in Port_SetPinMode API call are configured under PortPinMode</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>AUTOSAR_ECUC</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <DEFAULT-VALUE>false</DEFAULT-VALUE>
                            </ECUC-BOOLEAN-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinRbAllowOutputOverwrite -->
                            <ECUC-BOOLEAN-PARAM-DEF>
                              <SHORT-NAME>PortPinRbAllowOutputOverwrite</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Allow override by Output signal</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">This tag needs to be set to true for an input pin, to allow an additional output signal in parallel on the same physical port pin. By setting this tag to true the integrator confirms that an input and an output on the same pin is the intended configuration, and it is not a configuration error which would lead to a malfunction of the input configuration.</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>RB</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                            </ECUC-BOOLEAN-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinRbBootCtrlForward -->
                            <ECUC-BOOLEAN-PARAM-DEF>
                              <SHORT-NAME>PortPinRbBootCtrlForward</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Enable BootCtrl Forward</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">To be able to communicate with the voltage regulator on the PCB, rba_BootCrl needs to know which pins are used for communication with this device. This tag needs to be true for the following pins.<BR/>
<BR/>
SPI: SDO, SDI, CLK, CS<BR/>
MSC: SIP, SIN, FCLP, FCLN, SDO, SSY</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>RB</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                            </ECUC-BOOLEAN-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinRbCallbackEdgeSelection -->
                            <ECUC-ENUMERATION-PARAM-DEF>
                              <SHORT-NAME>PortPinRbCallbackEdgeSelection</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Callback edge selection</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">Selects on which edge of the signal a callback should be called</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>RB</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
                              <SYMBOLIC-NAME-VALUE>false</SYMBOLIC-NAME-VALUE>
                              <LITERALS>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_CALLBACK_EDGE_RISING</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">RISING</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>RB</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_CALLBACK_EDGE_FALLING</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">FALLING</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>RB</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                                <ECUC-ENUMERATION-LITERAL-DEF>
                                  <SHORT-NAME>PORT_PIN_CALLBACK_EDGE_BOTH</SHORT-NAME>
                                  <LONG-NAME>
                                    <L-4 L="EN">BOTH</L-4>
                                  </LONG-NAME>
                                  <ORIGIN>RB</ORIGIN>
                                </ECUC-ENUMERATION-LITERAL-DEF>
                              </LITERALS>
                            </ECUC-ENUMERATION-PARAM-DEF>
                            <!-- PARAMETER DEFINITION: PortPinRbCallbackFunctionName -->
                            <ECUC-STRING-PARAM-DEF>
                              <SHORT-NAME>PortPinRbCallbackFunctionName</SHORT-NAME>
                              <LONG-NAME>
                                <L-4 L="EN">Callback function name</L-4>
                              </LONG-NAME>
                              <DESC>
                                <L-2 L="EN">The name of the callback function to be called.<BR/>
This needs to be a void void function.<BR/>
Do not append () to the function name</L-2>
                              </DESC>
                              <LOWER-MULTIPLICITY>0</LOWER-MULTIPLICITY>
                              <UPPER-MULTIPLICITY>1</UPPER-MULTIPLICITY>
                              <MULTIPLICITY-CONFIG-CLASSES>
                                <ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-MULTIPLICITY-CONFIGURATION-CLASS>
                              </MULTIPLICITY-CONFIG-CLASSES>
                              <ORIGIN>RB</ORIGIN>
                              <POST-BUILD-VARIANT-MULTIPLICITY>true</POST-BUILD-VARIANT-MULTIPLICITY>
                              <POST-BUILD-VARIANT-VALUE>true</POST-BUILD-VARIANT-VALUE>
                              <VALUE-CONFIG-CLASSES>
                                <ECUC-VALUE-CONFIGURATION-CLASS>
                                  <CONFIG-CLASS>PRE-COMPILE</CONFIG-CLASS>
                                  <CONFIG-VARIANT>VARIANT-PRE-COMPILE</CONFIG-VARIANT>
                                </ECUC-VALUE-CONFIGURATION-CLASS>
                              </VALUE-CONFIG-CLASSES>
